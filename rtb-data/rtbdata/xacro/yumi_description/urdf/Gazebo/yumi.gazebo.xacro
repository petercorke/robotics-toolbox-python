<?xml version="1.0"?>

<robot name="yumi" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:macro name="yumi_gazebo" params="name">

    <!-- Gazebo reference frame -->
    <gazebo reference="${name}_body">
      <material>Gazebo/Ivory</material>
      <mu1>0.2</mu1>
      <mu2>0.2</mu2>
    </gazebo>


  </xacro:macro>

</robot>

